{
	"node": false,
	"es3": true,
	// "esnext": true,
	"browser": true,
	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"eqeqeq": true,
	"immed": true,
	"indent": 1,
	"latedef": true,
	"newcap": true,
	"noarg": true,
	"quotmark": "double",
	"regexp": true,
	"undef": true,
	"unused": true,
	"strict": true,
	"trailing": true,
	"smarttabs": false,
	"freeze": true,
	"maxparams": 5,
	"maxdepth": 4,
	"maxstatements": 15,
	"maxcomplexity": 6,
	"maxlen": 190,
	"globals": {
		"angular": false,
		"jQuery": false,
		"$": false,
		"_": false,
		"google": false,
		"d3": false,
		"moment": false
	}
}
